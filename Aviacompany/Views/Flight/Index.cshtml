@using Aviacompany.Library.Entities
@model IEnumerable<Flight>

@{
    ViewBag.Title = "Рейсы";
}

@foreach (var p in @Model)
{
    <div>
        <h3>@p.FlightNumber</h3>
        <p>@p.FlightDate.Date</p>
        <h4>@p.FlightFromCity.CityName</h4>
        <h4>@p.FlightToCity.CityName</h4>
    </div>
}